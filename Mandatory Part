ls, wc, awk, grep (and more)

echo
    -n

cd
    ./, ../, ~/

pwd
    no option

export , unset , env
    no options

exit
    no option

;

(Double Quote) ""
    $ excepted
    
(Single Quote) ''

(redirection) < , > , >>, << 

(pipe) |

Ctrl + C, D, \

(Environment variables) $

$? : return value
        0 (SUCCESS)
        1 (ERROR) : too many argument, not a valid identifier etc.
        2 (No such file or directory)
        126 (PERMISSION DENIED) : is a directory, command exist but cannot excute
        127 (COMMAND NOT FOUND)
        130 (script terminated by Ctrl + C)
        255 (Numeric argument required)
        ??? Invalid syntax error
    >> check error_mesage_list

input
"$SHLVL"    '$SHLVL'    "'$SHLVL'"    '"$SHLVL"'    '$SH"LVL'    "$SH'LVL"    "$"SHLVL    '$'SHLVL    $S'HLVL'    $S"HLVL"    $S'$SHLVL'    $S"$SHLVL"

output
3            $SHLVL        '3'        "$SHLVL"      $SH"LVL        'LVL        $SHLVL        $SHLVL        HLVL        HLVL        $SHLVL        3


"" : ignore most but $, `, \
'' : ignore everything (switch)